{
    "manifest_version": 2,
    "name": "Video Mirror",
    "version": "0.0.2",
    "description": "Flip videos to their original position",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "contextMenus",
        "tabs"
    ]
}